apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: mysql
  labels:
    app: my-wordpress
    tier: mysql
  annotations:
    sample-annotation: sample-value
info:
  emails:
    - kpt-team@google.com
  description: This is an example mysql package.
pipeline:
  mutators:
    - image: gcr.io/kpt-fn/set-labels:v0.1
      configMap:
        tier: mysql
